<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
        cookie_persistence="False" default_skin="Plone Default"
        request_varname="plone_skin">

  <object name="at_extensions" meta_type="Filesystem Directory View"
          directory="Products.ATExtensions:skins/at_extensions"/>

  <skin-path name="*">
    <layer name="at_extensions" insert-before="archetypes"/>
  </skin-path>

</object>
